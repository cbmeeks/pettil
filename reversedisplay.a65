basic		= $0400
		.word basic+1	; PET BASIC storage starts here
*		= basic+1
		.word basend
		.word 10
		.byt $9e
		.asc "(1039)",0	; $040f
basend		.word 0
main		jsr $f335
		bne main
main2		jsr $f335
		beq main2
		jsr flip
		beq main
;		rts
		

flip		ldy #0
flip1		lda $8000,y
		eor #$80
		sta $8000,y
		lda $8100,y
		eor #$80
		sta $8100,y
		lda $8200,y
		eor #$80
		sta $8200,y
		lda $8300,y
		eor #$80
		sta $8300,y
		iny
		bne flip1
		rts
