CLRSCR		= $93
time		= $8d	; jiffy clock 8d-8f, lowest zeropage used by kernel
errlnk		= $94	; NMI vector

cas1buf		= $027a
cas2buf		= $033c

; see "Programming The PET/CBM" p. 384
PIA1		= $E810
PIA1PORTA	= PIA1+0
PIA1CRA		= PIA1+1
PIA1PORTB	= PIA1+2
PIA1CRB		= PIA1+3

PIA2		= $E820
PIA2PORTA	= PIA2+0
PIA2CRA		= PIA2+1
PIA2PORTB	= PIA2+2
PIA2CRB		= PIA2+3

VIA		= $E840

; new ROM only
CONCAT		= $ff93
DOPEN		= $ff96
DCLOSE		= $ff99
RECORD		= $ff9c
HEADER		= $ff9f
COLLECT		= $ffa2
BACKUP		= $ffa5
COPY		= $ffa8
APPEND		= $ffab
DSAVE		= $ffae
DLOAD		= $ffb1
DIRECTORY	= $ffb4
RENAME		= $ffb7
SCRATCH		= $ffba
UNKNOWN		= $ffbd
; original ROM
OPEN		= $ffc0
CLOSE		= $ffc3
CHKIN		= $ffc6	; Set input device
CHKOUT		= $ffc9	; Set output device
CLRCHN		= $ffcc	; Restore default I/0 devices
CHRIN		= $ffcf	; Input character
CHROUT		= $ffd2	; Output character
LOAD		= $ffd5
SAVE		= $ffd8
SETTIM		= $ffdb ; VERIFY in original ROM ~ Is this still VERIFY?
RDTIM		= $ffde ; SYS in original ROM ~ Is this still SYS?
STOP		= $ffe1 ; GETSTOP in original ROM
GETIN		= $ffe4	; Get character 
CLALL		= $ffe7
UDTIM		= $ffea

NMIVEC		= $fffa
RESVEC		= $fffc
IRQVEC		= $fffe


